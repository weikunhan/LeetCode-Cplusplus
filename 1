class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<vector<int>> res;
        
        for (int i = 0; i < size.nums(); i++) {
            for (int j = i; j < size.nums(); j++) {
                if (nums[i] + nums[j] == target) {
                    res.push_back(i);
                    res.push_back(j);
                }
        }
        
        return res;
    }
};
